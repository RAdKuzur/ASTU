@model Lab2.Models.Museum

<h2>Добавить музей</h2>

<form asp-action="Create">
    <div class="form-group">
        <label asp-for="Name" class="control-label">Название</label>
        <input asp-for="Name" class="form-control" />
        <span asp-validation-for="Name" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Address" class="control-label">Адрес</label>
        <input asp-for="Address" class="form-control" />
        <span asp-validation-for="Address" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Exhibits" class="control-label">Выставки</label>
        <input asp-for="Exhibits" class="form-control" />
        <span asp-validation-for="Exhibits" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Person" class="control-label">Куратор</label>
        <input asp-for="Person" class="form-control" />
        <span asp-validation-for="Person" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Year" class="control-label">Год</label>
        <input asp-for="Year" class="form-control" />
        <span asp-validation-for="Year" class="text-danger"></span>
    </div>

    <div class="form-group">
        <input type="submit" value="Добавить" class="btn btn-primary" />
    </div>
    <a asp-action="Index">Назад</a>
</form>


 