@model Lab2.Models.Museum

<h2>Удалить музей</h2>

<h3>Вы уверены, что хотите это удалить?</h3>
<div>
    <h4>@Model.Name</h4>
    <p>@Model.Address</p>
    <p>@Model.Person</p>
    <p>@Model.Year</p>
</div>

<form asp-action="Delete">
    <input type="hidden" asp-for="Id" />
    <input type="submit" value="Delete" class="btn btn-danger" /> |
    <a asp-action="Index">Назад</a>
</form>
